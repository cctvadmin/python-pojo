
@font-face {
	font-family: texticons;
	font-weight: normal;
	font-style: normal;
	src: url('https://at.alicdn.com/t/font_984210_5cs13ndgqsn.ttf') format('truetype');
}
page,
view {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
page {
	background-color: #F6F7FB;
}
.center {
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
}
.logo {
	width: 750rpx;
	height: 240rpx;
	padding: 20rpx;
	box-sizing: border-box;
	background-color: #F6F7FB;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.logo-hover {
	opacity: 0.8;
}
.logo-img {
	width: 150rpx;
	height: 150rpx;
	border-radius: 150rpx;
}
.logo-title {
	height: 150rpx;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	margin-left: 20rpx;
}
.uer-name {
	height: 60rpx;
	line-height: 60rpx;
	font-size: 38rpx;
	color: #4E5A6E;
}
.go-login.navigat-arrow {
	font-size: 38rpx;
	color: #717FB4;
}
.login-title {
	height: 150rpx;
	-webkit-box-align: self-start;
	-webkit-align-items: self-start;
	        align-items: self-start;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	margin-left: 20rpx;
}
.center-list {
	background-color: #FFFFFF;
	margin-top: 20rpx;
	width: 750rpx;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
}
.center-list-item {
	height: 90rpx;
	width: 750rpx;
	box-sizing: border-box;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	padding: 0rpx 20rpx;
}
.border-bottom {
	border-bottom-width: 1rpx;
	border-color: #F6F7FB;
	border-bottom-style: solid;
}
.list-icon {
	width: 40rpx;
	height: 90rpx;
	line-height: 90rpx;
	font-size: 34rpx;
	color: #717FB4;
	text-align: center;
	font-family: texticons;
	margin-right: 20rpx;
}
.list-text {
	height: 90rpx;
	line-height: 90rpx;
	font-size: 34rpx;
	color: #4E5A6E;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	text-align: left;
}
.navigat-arrow {
	height: 90rpx;
	width: 40rpx;
	line-height: 90rpx;
	font-size: 34rpx;
	color: #4E5A6E;
	text-align: right;
	font-family: texticons;
}
.exit_login {
	position: absolute;
	width: 100%;
	bottom: 20px;
	text-align: center;
	color: #4E5A6E;
}

